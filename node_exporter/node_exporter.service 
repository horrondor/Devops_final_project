service 
[Unit]
Description=Node Exporter
Wants=network-online.target
After=network-online.target
[Service]
Type=simple
User=node_exporter
Group=node_exporter
ExecStart=/usr/local/bin/node_exporter \
  --web.config.file=/etc/node_exporter/config.yml
Restart=always

[Install]
WantedBy=multi-user.target